<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <appSettings>
    <!-- WindowsAzure.com URL -->
    <!--<add key="EnvironmentUrl" value="http://azure.microsoft.com"/>-->
    <add key="EnvironmentUrl" value="http://acom-int.azurewebsites.net"/>

    <!-- Browserstack Automate Credentials -->
    <!--<add key="BrowserStackUser" value="%BrowserStackUser%"/>
    <add key="BrowserStackKey" value="%BrowserStackKey%"/>-->

    <!-- Use below for running tests locally in browser stack-->
    <add key="BrowserStackUser" value="jamescon1"/>
    <add key="BrowserStackKey" value="zNpAXrC3BoxNppsZkBJs"/>

    <!-- Test Profiles to Run -->
    <add key="BrowserProfiles" value="Default"/>

    <!-- Set to true if you want to run tests marked with the [UnsupportedBrowserAttribute] -->
    <add key="IncludeUnsupportedBrowsers" value="false" />

    <!-- Project Name -->
    <add key="Project" value="WACOM" />

    <!-- Remote Hub Url (localhost:4444 to run it with local selenium server) -->
    <add key="seleniumserverhuburl" value="http://hub.browserstack.com/wd/hub/"/>

    <!-- Log timings of test actions and overall test duration -->
    <add key="LogTimings" value="False" />

    <!-- Set proxy address to use in browser -->
    <add key="ProxyAddress" value="127.0.0.1:8877" />

    <!-- Tagging tests: Set to true to disable navigation when clicking elements, preventing aborted requests -->
    <add key="DisableElementNavigation" value="false" />
  </appSettings>
</configuration>